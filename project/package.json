    "@storybook/vue": "^6.1.5",
    "axios": "^0.19",
    "babel-loader": "^8.2.1",
    "cross-env": "^7.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-plugin-vuejs-accessibility": "^0.6.0",
    "laravel-mix": "^5.0.1",
    "lodash": "^4.17.19",
